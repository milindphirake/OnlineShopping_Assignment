<p id="notice"><%= notice %></p>

<h1>Listing Shipper Addresses</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>District</th>
      <th>State</th>
      <th>Country</th>
      <th>Pincode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shipper_addresses.each do |shipper_address| %>
      <tr>
        <td><%= shipper_address.city %></td>
        <td><%= shipper_address.district %></td>
        <td><%= shipper_address.state %></td>
        <td><%= shipper_address.country %></td>
        <td><%= shipper_address.pincode %></td>
        <td><%= link_to 'Show', shipper_address %></td>
        <td><%= link_to 'Edit', edit_shipper_address_path(shipper_address) %></td>
        <td><%= link_to 'Destroy', shipper_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipper address', new_shipper_address_path %>
