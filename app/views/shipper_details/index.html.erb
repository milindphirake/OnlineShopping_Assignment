<p id="notice"><%= notice %></p>

<h1>Listing Shipper Details</h1>

<ul class="nav nav-tabs" role="tablist" >
 <li> <%= link_to 'Category', categories_path %></li>
  <li><%= link_to 'Company', companies_path %></li>
  <li  class="active"><%= link_to 'Shipper', shipper_details_path %></li>
  </ul>

<table class="table">
  <thead>
    <tr>
      <th>Fname</th>
      <th>Lname</th>
      <th>Email</th>
      <th>Mobile no</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shipper_details.each do |shipper_detail| %>
      <tr>
        <td><%= shipper_detail.fname %></td>
        <td><%= shipper_detail.lname %></td>
        <td><%= shipper_detail.email %></td>
        <td><%= shipper_detail.mobile_no %></td>
       
        <td><%= link_to 'Show', shipper_detail %></td>
        <td><%= link_to 'Edit', edit_shipper_detail_path(shipper_detail) %></td>
        <td><%= link_to 'Destroy', shipper_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipper detail', new_shipper_detail_path %>
